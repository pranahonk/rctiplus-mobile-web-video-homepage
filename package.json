  "scripts": {
    "dev": "cross-env NODE_ENV=DEVELOPMENT node ./server/index.js",
    "dev:3001": "cross-env NODE_ENV=development PORT=3001 node ./server/index.js",
    "dev:3002": "cross-env NODE_ENV=development PORT=3002 node ./server/index.js",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "npm run build && cross-env NODE_ENV=production node ./server/index.js",
    "start:3001": "npm run build && cross-env NODE_ENV=production PORT=3001 node ./server/index.js",
    "start:3002": "npm run build && cross-env NODE_ENV=production PORT=3002 node ./server/index.js",
    "run": "NODE_ENV=production node ./server/index.js",
    "run:3001": "NODE_ENV=production PORT=3001 node ./server/index.js",
    "run:3002": "NODE_ENV=production PORT=3002 node ./server/index.js",
    "start:static": "npm run export && cross-env NODE_ENV=production node ./server/static",
    "deploy": "now --public",
    "cypress:open": "cypress open",
    "installllll": "yarn build-all",
    "next": "yarn build-firebase && yarn && yarn dev",
    "preserve": "yarn build-all",
    "serve": "firebase serve",
    "predeploy": "yarn build-all",
    "firebase:deploy": "firebase deploy",
    "build-all": "yarn build-next && yarn build-firebase",
    "build-next": "yarn && yarn build",
    "build-firebase": "cd \"functions\" && yarn",
    "lighthouse:ci": "node node_modules/lighthouse/lighthouse-cli/index.js --output-path=./lighthouse-report.html --quiet --chrome-flags='--headless' https://rc-m-new.rctiplus.com",
    "push:rc": "git checkout az-sprint2 && git add . && git commit -m \"preparation for production\" && git push origin az-sprint2 && git checkout release-candidates && git merge az-sprint2 && git push origin release-candidates && git checkout az-sprint2",
    "push:prod": "git checkout az-sprint2 && git add . && git commit -m \"bug fix\" && git push origin az-sprint2 && git checkout production && git merge az-sprint2 && git push origin production && git checkout az-sprint2"
  },
